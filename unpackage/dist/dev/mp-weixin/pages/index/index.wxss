
.container {
	padding: 50rpx 30rpx;
}
.phone-wrap {
	text-align: center;
	color: #333;
}
.phone {
	line-height: 50rpx;
	font-weight: bold;
	font-size: 50rpx;
}
.belong {
	margin-top: 20rpx;
	line-height: 40rpx;
	font-size: 40rpx;
}
.salutation-wrap {
	margin-top: 50rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 32rpx;
}
.salutation-label {
	font-size: 32rpx;
}
.salutation-input {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	text-align: left;
	font-size: 32rpx;
	height: 50rpx;
	color: #333333;
	border-bottom: 0.5px solid #e2e2e2;
}
.btn-wrap {
	margin-top: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.btn {
	margin: 10rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.btn:first-child {
	margin-left: 0;
}
.btn:last-child {
	margin-right: 0;
}
.template-content {
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	margin-top: 20rpx;
	display: block;
	height: 400rpx;
	width: 100%;
	padding: 10rpx;
	-webkit-border-radius: 10rpx;
	        border-radius: 10rpx;
	background-color: #F8F8F8;
}

