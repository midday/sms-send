<view class="content"><view class="header"><image src="/static/logo.png"></image></view><view class="list"><view class="list-call"><image class="img" src="/static/phone.png"></image><input class="sl-input" type="number" maxlength="11" placeholder="输入手机号" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['userInfo']]]]]}}" value="{{userInfo.username}}" bindinput="__e"/></view><view class="list-call"><image class="img" src="/static/password.png"></image><input class="sl-input" type="text" maxlength="32" placeholder="输入密码" password="true" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['userInfo']]]]]}}" value="{{userInfo.password}}" bindinput="__e"/></view></view><view class="button-login" hover-class="button-hover" data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" bindtap="__e">登录</view></view>